{
	"name": "LS Wing",
	"type": "OSC",
	"path": "Hardware",
	
	"version": "1.0",
	"description": "Allow you to control a LightShark's LS-Wing with Chataigne",
	"url":"https://github.com/ziginfo/LS-Wing-OSC-Chataigne-Module",
	"downloadURL": "https://github.com/ziginfo/LS-Wing-OSC-Chataigne-Module/archive/refs/heads/main.zip",
	
	"hasInput": true,
	"hasOutput": true,

	"hideDefaultCommands": true,
	"hideDefaultParameters": [],

	"defaults": {
		"OSC Input": {
			"localPort" : 9000 },
		"OSC Outputs": {
			"OSC Output": {
				"local" : false,
				"remoteHost" : "lswing.local",
				"remotePort" : 8000,
				"listenToFeedback":false  }  }
	},
	
	"parameters": {
	},
	
	"scripts": [ "lswing.js" ],
	
	"values": {
	},
	
	
	
"commands": {


	
	"Push Button" : {
			"menu" : "Control",
			"callback" : "push_button",
			"parameters" : {
			
				"Page": {
					"type": "Enum",
					"options" : {
						"Page1" : 1,
						"Page2" : 2 } }, 
					
				"Select Row (1-6)": {
					"type": "Enum",
					"options" : {
						"1 Row1" : 1,
						"2 Row2" :2,
						"3 Select" : 3,
						"4 Play" :4,
						"5 Pause" : 5,
						"6 Mute/Flash" :6  } },
					
				"Button N° (1-10)": {
					"type": "Integer",
					"default": 1, 
					"min":1,
					"max":10,
					"useForMapping": true },  
					
				"Choice": {
					"type": "Enum",
					"options" : {
						"Press" : 1,
						"Release" :0 } }		
			} },
			
		"Toggle Button" : {
			"menu" : "Control",
			"callback" : "toggle_button",
			"parameters" : {
			
				"Page": {
					"type": "Enum",
					"options" : {
						"Page1" : 1,
						"Page2" : 2 } }, 
					
				"Select Row (1-6)": {
					"type": "Enum",
					"options" : {
						"1 Row1" : 1,
						"2 Row2" :2,
						"3 Select" : 3,
						"4 Play" :4,
						"5 Pause" : 5,
						"6 Mute/Flash" :6  } },
					
				"Button N° (1-10)": {
					"type": "Integer",
					"default": 1, 
					"min":1,
					"max":10,
					"useForMapping": true }  
					
			} },
			
		"Focus Push Button" : {
			"menu" : "Control",
			"callback" : "focus_button",
			"parameters" : {
					
				"Select Row (1-6)": {
					"type": "Enum",
					"options" : {
						"1 Row1" : 1,
						"2 Row2" :2,
						"3 Select" : 3,
						"4 Play" :4,
						"5 Pause" : 5,
						"6 Mute/Flash" :6  } },
					
				"Button N° (1-10)": {
					"type": "Integer",
					"default": 1, 
					"min":1,
					"max":10,
					"useForMapping": true },  
					
				"Choice": {
					"type": "Enum",
					"options" : {
						"Press" : 1,
						"Release" :0 } }		
			} },
			
			
		"Fader Level" : {
			"menu" : "Control",
			"callback" : "fader",
			"parameters" : {
			
				"Select Page": {
					"type": "Enum",
					"options" : {
						"Page1" : 1,
						"Page2" : 2 } }, 
					
				"Fader N°": {
					"type": "Integer",
					"default": 1, 
					"min":1,
					"max":10,
					"useForMapping": true }, 
						
				"Level": {
					"type": "Float",
					"default": 0, 
					"min":0,
					"max":1,
					"useForMapping": true } 			
			} },
			
		"Focus Fader Level" : {
			"menu" : "Control",
			"callback" : "focus_fader",
			"parameters" : {
					
				"Fader N°": {
					"type": "Integer",
					"default": 1, 
					"min":1,
					"max":10,
					"useForMapping": true }, 
						
				"Level": {
					"type": "Float",
					"default": 0, 
					"min":0,
					"max":1,
					"useForMapping": true } 			
			} },
			
		
		"Select Page" : {
			"menu" : "Action",
			"callback" : "page",
			"parameters" : {
					"Select Page": {
					"type": "Enum",
					"options" : {
						"Page1" : 1,
						"Page2" : 2 } } 					
			} },
					
			
		"Sync" : {
			"menu" : "Action",
			"callback" : "sync"
			},
			
		"Sync Faders" : {
			"menu" : "Action",
			"callback" : "sync_faders"
			},
			
		"Sync Buttons" : {
			"menu" : "Action",
			"callback" : "sync_buttons"
			},
			
		"Reset All" : {
			"menu" : "Action",
			"callback" : "reset"
			},
			
		"Reboot" : {
			"menu" : "Action",
			"callback" : "reboot"
			},

	}
}
